<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import com.jfoenix.controls.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.view.MainLayoutController">
   <children>
      <JFXDrawer fx:id="drawer" defaultDrawerSize="200.0" layoutX="-1.0" prefHeight="630.0" prefWidth="205.0" />
      <JFXHamburger fx:id="hamburger" layoutX="14.0" layoutY="14.0" />
      <VBox layoutX="204.0">
         <children>
            <StackPane alignment="TOP_RIGHT">
               <children>
                  <Label onMouseClicked="#close" text="X">
                     <font>
                        <Font name="System Bold" size="18.0" />
                     </font>
                  </Label>
               </children>
               <VBox.margin>
                  <Insets right="5.0" />
               </VBox.margin>
            </StackPane>
            <StackPane>
               <children>
                  <AnchorPane fx:id="chaträumeAnchor" prefHeight="200.0" prefWidth="200.0" visible="false">
                     <children>
                        <fx:include source="Chaträume.fxml" />
                     </children>
                  </AnchorPane>
                  <AnchorPane fx:id="gruppenAnchor" prefHeight="200.0" prefWidth="200.0" visible="false">
                     <children>
                        <fx:include source="Gruppen.fxml" />
                     </children>
                  </AnchorPane>
                  <AnchorPane fx:id="terminplanerAnchor" prefHeight="200.0" prefWidth="200.0">
                     <children>
                        <fx:include source="Terminplaner.fxml" />
                     </children>
                  </AnchorPane>
                  <AnchorPane fx:id="notizenAnchor" prefHeight="200.0" prefWidth="200.0" visible="false">
                     <children>
                        <fx:include source="Notizen.fxml" />
                     </children>
                  </AnchorPane>
                  <AnchorPane fx:id="spielebibliothekAnchor" layoutX="204.0" visible="false">
                     <children>
                        <fx:include source="Spielebibliothek.fxml" />
                     </children>
                  </AnchorPane>
               </children>
            </StackPane>
         </children>
      </VBox>
   </children>
</AnchorPane>
